{
    "ecmaFeatures": {
        "modules": true,
        "jsx": true
    },
    "env": {
        "browser": true,
        "es6": true,
        "mocha": true,
        "jasmine": true,
        "node": true
    },
    "rules": {
        "block-scoped-var": 2,
        "brace-style": [2, "1tbs"],
        "camelcase": 2,
        "comma-dangle": [2, "never"],
        "comma-spacing": [2, {"before": false, "after": true}],
        "comma-style": [2, "last"],
        "complexity": [0, 0],
        "consistent-return": 2,
        "curly": 2,
        "default-case": 2,
        "dot-notation": 2,
        "eol-last": 2,
        "eqeqeq": 2,
        "guard-for-in": 2,
        "indent": [2, 4],
        "key-spacing": [2, {"beforeColon": false, "afterColon": true}],
        "new-cap": [2, {"newIsCap": true, "capIsNew": false}],
        "new-parens": 2,
        "no-alert": 2,
        "no-array-constructor": 2,
        "no-bitwise": 2,
        "no-caller": 2,
        "no-catch-shadow": 2,
        "no-cond-assign": [2, "always"],
        "no-constant-condition": 1,
        "no-control-regex": 2,
        "no-debugger": 2,
        "no-delete-var": 2,
        "no-div-regex": 2,
        "no-dupe-keys": 2,
        "no-duplicate-case": 2,
        "no-else-return": 2,
        "no-empty": 2,
        "no-empty-character-class": 2,
        "no-labels": 2,
        "no-eq-null": 2,
        "no-eval": 2,
        "no-ex-assign": 2,
        "no-extend-native": 2,
        "no-extra-bind": 2,
        "no-extra-parens": [2, "functions"],
        "no-extra-semi": 2,
        "no-fallthrough": 2,
        "no-floating-decimal": 2,
        "no-func-assign": 2,
        "no-implied-eval": 2,
        "no-inner-declarations": 2,
        "no-invalid-regexp": 2,
        "no-irregular-whitespace": 2,
        "no-iterator": 2,
        "no-label-var": 2,
        "no-lone-blocks": 2,
        "no-loop-func": 2,
        "no-multi-str": 2,
        "no-multiple-empty-lines": [2, {"max": 2}],
        "no-native-reassign": 2,
        "no-nested-ternary": 2,
        "no-new": 2,
        "no-new-func": 2,
        "no-new-object": 2,
        "no-new-wrappers": 2,
        "no-obj-calls": 2,
        "no-octal": 2,
        "no-octal-escape": 2,
        "no-plusplus": 2,
        "no-proto": 2,
        "no-redeclare": 2,
        "no-regex-spaces": 2,
        "no-return-assign": [ 2, "except-parens" ] ,
        "no-script-url": 2,
        "no-self-compare": 2,
        "no-sequences": 2,
        "no-shadow": 2,
        "no-shadow-restricted-names": 2,
        "no-sparse-arrays": 2,
        "no-sync": 2,
        "no-throw-literal": 2,
        "no-trailing-spaces": 2,
        "no-undef": 2,
        "no-undef-init": 2,
        "no-underscore-dangle": 2,
        "no-unreachable": 2,
        "no-unused-expressions": 2,
        "no-unused-vars": [2, {"vars": "local", "args": "after-used"}],
        "no-use-before-define": 2,
        "no-with": 2,
        "one-var": [2, "always"],
        "quotes": [2, "single", "avoid-escape"],
        "requirejs/no-invalid-define": 2,
        "requirejs/no-multiple-define": 2,
        "requirejs/no-commonjs-wrapper": 2,
        "requirejs/no-object-define": 1,
        "radix": 2,
        "semi": [2, "always"],
        "semi-spacing": [2, {"before": false, "after": true}],
        "keyword-spacing": 2,
        "space-before-blocks": [2, "always"],
        "space-before-function-paren": [2, { "anonymous": "always", "named": "never" }],
        "space-infix-ops": 2,
        "strict": [0, "global"],
        "use-isnan": 2,
        "valid-jsdoc": 1,
        "valid-typeof": 2,
        "vars-on-top": 2,
        "wrap-iife": [2, "any"],
        "wrap-regex": 2,
        "yoda": [2, "never"],
        "constructor-super": 2,
        "no-this-before-super": 2,
        "no-var": 2,
        "prefer-const": 1,
        "no-const-assign": 2
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "requirejs"
    ],
    "globals": {
        "d3": true,
        "moment": true,
        "define": true,
        "React": true,
        "ReactDOM": true
    }
}